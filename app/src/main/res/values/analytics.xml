<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="TypographyDashes">

    <!-- debug Google Analytics (prints in the logs) -->
    <bool name="ga_debug">false</bool>

    <string name="ga_trackingId">UA-40620268-1</string>

    <!-- Enable automatic activity tracking -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!-- Enable automatic exception tracking -->
    <bool name="ga_reportUncaughtExceptions">true</bool>
    
    <!-- The dispatch period in seconds. Defaults to 30 minutes. 
    	86400 = 24hours -->
    <integer name="ga_dispatchPeriod">86400</integer>
    
	<!-- The amount of time (in seconds) your application can stay
	 in the background before the session is ended. Default is 30 seconds. 
	 Negative value disables EasyTracker session management. -->
    <integer name="ga_sessionTimeout">120</integer>

</resources>
